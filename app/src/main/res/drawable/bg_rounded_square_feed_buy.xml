<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/feedBorder2" />

            <corners
                android:bottomRightRadius="@dimen/scale_10dp"
                android:topRightRadius="@dimen/scale_10dp" />
        </shape>
    </item>

    <item
        android:bottom="@dimen/scale_1dp"
        android:right="@dimen/scale_1dp"
        android:top="@dimen/scale_1dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/feedColor2" />

            <corners
                android:bottomRightRadius="@dimen/scale_10dp"
                android:topRightRadius="@dimen/scale_10dp" />
        </shape>
    </item>


</layer-list>